<% @warning.each do |warning| %>
  <li class="custom_notifications">
    <div class="notifyReport report-<%= option_class(warning.user_read) %>"
      id="notify-id-<%= warning.id %>" data-id="<%= warning.id %>">
      <div class="author-thumb">
        <%= image_tag warning.club_logo %>
      </div>
      <div class="notification-event">
        <div class="notification-value">
          <span class="h6 notification-friend"><%= warning.club_name %></span>
          <%= t("of_manager") + t("warning_reports.msg_not_report") %>
          <span class="notification-link"><%= type_report warning %>-
            <%= t "year" %> <%= warning.year %></span>
            <% if warning.deadline.present? %>
              <%= t "warning_reports.deadline" %>
              <%= l warning.deadline, format: :short %>
            <% end %>
            </br><i class="fa fa-clock-o time_create" aria-hidden="true">
              <%= l warning.created_at, format: :short %></i>
        </div>
      </div>
    </div>
  </li>
<% end %>
