<% if @organization %>
  $('#js-list-data-setting').html('<%= j render partial: "list_rules", locals: {rules: @rules} %>');
  $('#js-list-data-setting').fadeIn();
<% else %>
  $('.container').append('<%= j render "shared/flash_warning" %>')
  $('.notify').slideUp(3000);
<% end %>
$('#search-organization').empty();
