<% if @clubs %>
  $('#organization_club').html('<%= j render partial: "clubs/clubs", locals: {clubs: @clubs} %>');
  if ($('#search-organization').find('#organization_club_form').length === 0){
    $('#search-organization').html('<%= j render "search_club" %>');
  }
<% else %>
  $('#organization_club').html('<%= j render partial: "timeline",
    locals: {events: @organization_event} %>');
<% end %>
$('#club_report_form').remove();
