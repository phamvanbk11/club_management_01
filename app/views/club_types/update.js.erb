<% if @club_type.errors.any? %>
  $('#errors-edit').html('<%= j render partial: "shared/error_messages",
    locals: {object: @club_type} %>')
<% else %>
  $('#js-list-data-setting').html('<%= j render partial: "list_types",
    locals: {club_types: @club_types} %>');
<% end %>
$('.container').append('<%= j render partial: "shared/flash_warning" %>');
